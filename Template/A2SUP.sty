% Introduction d'une question, de son énoncé, accompagné d'aides au calcul, et des 5 items
% \questionavecaide{num question}{énoncé}{aide}{A}{B}{C}{D}{E}
\newcommand{\questionavecaide}[8]
{%
    %\addtocounter{numqst}{1} % Incrémentation pour numérotation automatique
    \global\def\numquestioncorrection{#1}%
    
    \begin{minipage}{\linewidth}%
	\vspace{.5cm}%

    %%%%%%%%%%%%%%%%%%%%%%%%%%%
    %%  MEP Enoncé question  %%
    %%%%%%%%%%%%%%%%%%%%%%%%%%%
    \MEPquestion{#1}{#2}%

    \leftskip=0cm%
    
	\medskip%

    \leftskip=2.5em %
	
    \textit{Aide aux calculs : #3}\\[-7pt]%
    
    \leftskip=0em %
    
    \IfSubStr{\cmdcle@sujet@SujetOuCorrection}{ujet}{% Si sujet
    \begin{enumerate}[label=\Alph*.] %
		\leftskip=\indentationitemsujet%
		\item #4%
		\item #5%
		\item #6%
		\item #7%
		\item #8%
	\end{enumerate}%
    
    \vspace{0.5cm}%
    
    }{%
    \global\def\bilan{}%

    \global\def\argI{{#4}}%
    \global\def\argII{{#5}}%
    \global\def\argIII{{#6}}%
    \global\def\argIV{{#7}}%
    \global\def\argV{{#8}}%
    }%
}



























