% Introduction d'une question et de son énoncé
% \question{1}{Enonce de la question}{Item A}{Item B}{Item C}{Item D}{Item E}
\newcommand{\question}[7]
{%
    %\addtocounter{numqst}{1} % Incrémentation pour numération automatique (pas d'intérêt actuellement)
    \global\def\numquestioncorrection{#1}%
    
    \begin{minipage}{\linewidth}%
	\vspace{.5cm}%

    %%%%%%%%%%%%%%%%%%%%%%%%%%%
    %%  MEP Enoncé question  %%
    %%%%%%%%%%%%%%%%%%%%%%%%%%%
    \MEPquestion{#1}{#2}%
    
    \leftskip=0cm%
    
	\medskip%

    \IfSubStr{\cmdcle@sujet@SujetOuCorrection}{ujet}{% Si sujet
    \begin{enumerate}[label=\Alph*.]%
		\leftskip=\indentationitemsujet%
		\item #3%
		\item #4%
		\item #5%
		\item #6%
		\item #7%
	\end{enumerate}%
    
    \vspace{0.5cm}%
    
    }{%
    \global\def\bilan{} % réinitialisation

    % Utiles pour exploiter les items dans d'autres commandes, comme \mesitems et \vraifaux
    \global\def\argI{{#3}}%
    \global\def\argII{{#4}}%
    \global\def\argIII{{#5}}%
    \global\def\argIV{{#6}}%
    \global\def\argV{{#7}}%
    }
}






























